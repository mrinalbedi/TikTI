@{
    ViewData["Title"] = "Home Page";
}

<div class="container" style="max-width:100%; padding:0">
    <div class="carousel slide carousel-fade" id="featured" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#featured" data-slide-to="0"></li>
            <li data-target="#featured" data-slide-to="1"></li>
            <li data-target="#featured" data-slide-to="2" class="active"></li>
        </ol>
        <div class="carousel-inner" style="padding-top:0.3rem;">
            <div class="carousel-item active img-fluid" style="opacity:0.80">
                <img class="d-block w-100 image_height" src="~/Images/Job Seeker/1A.jpg" alt="Mission">
                <div class="slider_text">
                    <h3>Stop applying to jobs, let TikTi bring your ideal job to you</h3>
                    <p><input type="button" class="btn btn-primary btn-lg" value="Get Started"></p>
                </div>

            </div>
            <div class="carousel-item img-fluid">
                <img class="d-block w-100 image_height" src="~/Images/Job Seeker/2B.jpg" alt="Lifestyle Photo">
                <div class="slider_text">
                    <h3>Stop applying to jobs, let TikTi bring your ideal job to you</h3>
                    <p><input type="button" class="btn btn-primary btn-lg" value="Get Started"></p>
                </div>

            </div>
            <div class="carousel-item img-fluid">
                <img class="d-block w-100 image_height" src="~/Images/Job Seeker/3A.jpg" alt="Vaccinations">
                <div class="slider_text">
                    <h3>Stop applying to jobs, let TikTi bring your ideal job to you</h3>
                    <p><input type="button" class="btn btn-primary btn-lg" value="Get Started"></p>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#featured"
           role="button" data-slide="prev">
            <span class="carousel-control-prev-icon"
                  aria-hidden="true">
                <span class="sr-only">Previous</span>
            </span>
        </a>
        <a class="carousel-control-next" href="#featured"
           role="button" data-slide="next">
            <span class="carousel-control-next-icon"
                  aria-hidden="true">
                <span class="sr-only">Next</span>
            </span>
        </a>
    </div>
</div>
