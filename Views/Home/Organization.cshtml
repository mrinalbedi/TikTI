
@{
    ViewData["Title"] = "Organization";
    Layout = "~/Views/Shared/_Organization_view.cshtml";
}
<div class="container" style="max-width:100%; padding:0">
    <div class="carousel slide carousel-fade" id="featured" data-ride="carousel">
        <ol class="carousel-indicators">
            <li data-target="#featured" data-slide-to="0"></li>
            <li data-target="#featured" data-slide-to="1"></li>
            <li data-target="#featured" data-slide-to="2" class="active"></li>
        </ol>
        <div class="carousel-inner" style="padding-top:0.3rem;">
            <div class="carousel-item active img-fluid">
                <img class="d-block w-100" src="~/Images/Organization/1A.jpg" alt="Mission">
                <div class="slider_text">
                    <h3 class="font-weight-bolder">Get Qualified, Matched Candidates Quickly</h3>
                    <p><a class="btn btn-primary btn-lg" asp-controller="OrgRegister" asp-action="Register">Get Started</a></p>
                </div>

            </div>
            <div class="carousel-item img-fluid">
                <img class="d-block w-100" src="~/Images/Organization/1C.jpg" alt="Lifestyle Photo">
                <div class="slider_text">
                    <h3 class="font-weight-bolder">Get Qualified, Matched Candidates Quickly</h3>
                    <p><a class="btn btn-primary btn-lg" asp-controller="OrgRegister" asp-action="Register">Get Started</a></p>
                </div>

            </div>
            <div class="carousel-item img-fluid">
                <img class="d-block w-100" src="~/Images/Organization/1B.jpg" alt="Vaccinations">
                <div class="slider_text">
                    <h3 class="font-weight-bolder">Get Qualified, Matched Candidates Quickly</h3>
                    <p><a class="btn btn-primary btn-lg" asp-controller="OrgRegister" asp-action="Register">Get Started</a></p>
                </div>
            </div>
        </div>
        <a class="carousel-control-prev" href="#featured"
           role="button" data-slide="prev">
            <span class="carousel-control-prev-icon"
                  aria-hidden="true">
                <span class="sr-only">Previous</span>
            </span>
        </a>
        <a class="carousel-control-next" href="#featured"
           role="button" data-slide="next">
            <span class="carousel-control-next-icon"
                  aria-hidden="true">
                <span class="sr-only">Next</span>
            </span>
        </a>
    </div>
</div>
