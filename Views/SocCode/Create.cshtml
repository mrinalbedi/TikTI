@model Tikti.Models.SocCode

@{
    ViewData["Title"] = "Select SOC Code";
    Layout = "~/Views/Shared/_Organization_view.cshtml";
}
<hr />
<div class="row no_margin">
    <div class="col text-center">
        <h1 class="text-center" style="font-weight:500; font-variant:small-caps; font-size:3.5rem">
        "Let's Get Started"</h1>
    </div>
</div>
<hr/>
<div class="row no_margin">
    <div class="col text-center">
        <h2>Please select the SOC Code</h2>
    </div>
</div>

<form asp-action="Create">
    <div class="row no_margin">
        <div class="col-md-4"></div>
        <div class="col-md-4">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SocCode1" class="control-label"><br /></label>
                <select style="height:3rem" asp-for="SocCode1" class="form-control" asp-items="ViewBag.SocCode" id="wk"></select>
            </div>
        </div>
    </div>
    <div class="row no_margin">
        <div class="col text-center">
            <div class="form-group">
                <input type="submit" value="Next Step" class="btn btn-primary btn-lg" />
            </div>
        </div>
    </div>
    @*<div class="form-group">
        <input type="submit" value="Next Step" class="btn btn-primary btn-lg" />
    </div>*@
</form>
