            <td>
                @Html.DisplayFor(modelItem => item.HrFirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HrLastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HrTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HrDepartment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HrPhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HrEmail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RegistrationId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RegistrationId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RegistrationId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
