                @Html.DisplayNameFor(model => model.HrLastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HrTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HrDepartment)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HrPhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HrEmail)
            </th>
            <th></th>
        </tr>
